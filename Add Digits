class Solution {
    public int addDigits(int num) {
        
        while(num>=10)
        {
            int res,sum=0;
            {
                while(num>0)
                {
                    res=num%10;
                    sum+=res;
                    num=num/10;
                }
                num=sum;
            }
        }
        return num;
        }
}
